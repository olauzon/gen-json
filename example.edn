{
  ;; see https://github.com/clojure/data.generators/blob/master/src/main/clojure/clojure/data/generators.clj
  :data {
    :uuid   (gen/uuid)
    :num    (+ 40 (rand-int 60))
    :date   (gen/date)
    :letter (gen/one-of "a" "b" "c" "d")
    :bool   (gen/boolean)
    :short  (gen/short)
    :int    (gen/int)
    :long1  (gen/long)
    :string (gen/string)
    :long2  (gen/long)
    :ten    10
  }
  :output-file "hello.json"
  :lines 10
}
